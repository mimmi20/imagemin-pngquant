{
	"name": "imagemin-pngquant",
	"version": "10.0.0",
	"description": "Imagemin plugin for `pngquant`",
	"license": "MIT",
	"repository": "imagemin/imagemin-pngquant",
	"type": "module",
	"exports": {
		"types": "./index.d.ts",
		"default": "./index.js"
	},
	"engines": {
		"node": "^20.19.0 || ^22.12.0 || >= 24.0.0"
	},
	"scripts": {
		"test": "npm run test:cover && tsc --noEmit index.d.ts",
		"test:cover": "c8 --check-coverage --statements 90 ava",
		"prettier": "prettier --config prettier.config.mjs --check index.js test.js eslint.config.mjs prettier.config.mjs",
		"prettier-fix": "prettier --config prettier.config.mjs --write index.js test.js eslint.config.mjs prettier.config.mjs",
		"eslint": "eslint . --config eslint.config.mjs",
		"eslint-fix": "eslint . --config eslint.config.mjs --fix"
	},
	"files": [
		"index.js",
		"index.d.ts"
	],
	"keywords": [
		"compress",
		"image",
		"imageminplugin",
		"minify",
		"optimize",
		"png",
		"pngquant"
	],
	"dependencies": {
		"environment": "^1.1.0",
		"execa": "^9.6.0",
		"is-png": "^3.0.1",
		"ow": "^2.0.0",
		"pngquant-bin": "github:mimmi20/pngquant-bin#main",
		"uint8array-extras": "^1.4.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.27.0",
		"@types/node": "^22.15.30",
		"ava": "^6.1.3",
		"c8": "^10.1.3",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.4.0",
		"globals": "^16.2.0",
		"is-progressive": "^5.0.1",
		"prettier": "^3.5.3",
		"prettier-eslint": "^16.4.2",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.33.0"
	},
	"overrides": {
		"eslint": "$eslint"
	}
}
